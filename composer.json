{
    "name": "yiisoft/app",
    "type": "project",
    "description": "Yii 3 Project Template",
    "keywords": [
        "yii3",
        "app"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/app"
    },
    "require": {
        "php": "^8.1",
        "yiisoft/yii2-redis": "^3.0",
        "yiisoft/yii2-faker": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "serve": "rr serve"
    }
}
